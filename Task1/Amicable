int main()
{
    int a, b, s1 = 1, s2 = 1, h, i;
    cout << "Enter the first number: ";
    cin >> a;
    cout << "Enter the second number: ";
    cin >> b;

    if (a > b)
        h = a;
    else
        h = b;

    for (i = 2; i <= h / 2; i++)
    {
        if (a % i == 0)
            s1 += i;
        if (b % i == 0)
            s2 += i;
    }

    if (s1 == b && s2 == a)
        cout << "Amicable Number" << endl;
    else
        cout << "Not Amicable" << endl;

    return 0;
}
